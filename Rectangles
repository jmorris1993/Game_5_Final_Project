# -*- coding: utf-8 -*-


from UniVars import *
from graphics import *

class Rectangles (Rectangle):
    def __init__(self, p1, p2):
        Rectangle.__init__(self,p1,p2)
        


    def is_thing (self):
        return false

    def get_x(self):
    	pt = self.getCenter()
    	return pt.getX()

    def get_y(self):
    	pt = self.getCenter()
    	return pt.getY()

